<!DOCTYPE html>
<html>
<head>
	<title>Dis Bitch</title>
</head>

<body>
	<p><%= @artist.name %></p>
	<p>
		<% @artist.genres.each do |genre| %>
			<a href="/genres/<%= genre.name %>.html/"><%= genre.name %></a>
		<%end %>
	</p>
	<p>
		<% @artist.songs.each do |song| %>
			<a href="/songs/<%= song.to_slug %>.html/"><%= @artist.name %> - <%= song.name %> - <%= song.genre.name%>/</a>
		<%end %>		
	</p>
</body>
</html>